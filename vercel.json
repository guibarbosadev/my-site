      "destination": "https://streak-calendar.vercel.app"
    },
    {
      "source": "/streak-calendar/",
      "destination": "https://streak-calendar.vercel.app"
    },
    {
      "source": "/streak-calendar/:match",
      "destination": "https://streak-calendar.vercel.app"
    },
    {
      "source": "/streak-calendar-assets/:match*",
      "destination": "https://streak-calendar.vercel.app/streak-calendar-assets/:match*"
    }
  ]
}
